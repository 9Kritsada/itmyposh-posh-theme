{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#ffffff",
					"properties": {
					  "macos": "\uf179 ",
					  "ubuntu": "\uf31b ",
					  "windows": "\uf871 "
					},
					"style": "diamond",
					"template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
					"type": "os"
				},
				{
					"foreground": "#ab5dee",
					"style": "plain",
					"template": "{{ .UserName }} ",
					"type": "session"
				},
				{
					"foreground": "#1ddd33",
					"properties": {
						"style": "folder"
					},
					"style": "plain",
					"template": "<#cccccc>\uf838</> {{ .Path }} ",
					"type": "path"
				},
				{
					"foreground": "#c5c5c5",
					"style": "plain",
					"template": "<#30a4dc>\ufa9e</>",
					"type": "git"
				},
				{
					"background": "#1ddd33",
					"foreground": "#000",
					"properties": {
						"branch_icon": "\uf09b "
					},
					"style": "plain",
					"template": "<#000> </>{{ .HEAD }}<#000> </>",
					"type": "git"
				},
				{
					"foreground": "#ffffff",
					"style": "plain",
					"template": " \uf489 {{.Name}}\u2800",
					"type": "shell"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#1ddd33",
					"properties": {
						"always_enabled": true
					},
					"style": "plain",
					"template": "\uf155",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}